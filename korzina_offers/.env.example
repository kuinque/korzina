# Flask настройки
FLASK_ENV=development
DEBUG=True
HOST=0.0.0.0
PORT=5000

# Supabase настройки
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_key_here

# Логирование
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# API настройки
API_VERSION=v1
CORS_ORIGINS=*

# Бизнес логика
PENALTY_PRICE=1000.0
MIN_SIMILARITY_THRESHOLD=0.6
